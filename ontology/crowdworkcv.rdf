<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY ns "http://rdfs.org/sioc/ns#" >
    <!ENTITY wot "http://xmlns.com/wot/0.1/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY resume-rdf "http://rdfs.org/resume-rdf/#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY vs "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
]>


<rdf:RDF xmlns="http://west.uni-koblenz.de/ontologies/crowdworkcv#"
     xml:base="http://west.uni-koblenz.de/ontologies/crowdworkcv"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://rdfs.org/sioc/ns#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:resume-rdf="http://rdfs.org/resume-rdf/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#">
        <dc:contributor>Cristina Sarasua (WeST, University of Koblenz-Landau)</dc:contributor>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
        <dc:description xml:lang="en">A vocabulary to describe work experience in microtask crowdsourcing</dc:description>
        <dc:rights xml:lang="en">CC BY-SA (Creative Commons Attribution ShareAlike)</dc:rights>
        <dc:title xml:lang="en">Crowd Work CV</dc:title>
        <rdfs:seeAlso xml:lang="en">https://github.com/criscod/CrowdWorkCV</rdfs:seeAlso>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&rdfs;seeAlso"/>
    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    <owl:AnnotationProperty rdf:about="&rdfs;isDefinedBy"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&dc;description">
        <rdfs:label xml:lang="en-us">Description</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://rdfs.org/sioc/ns#account_of -->

    <owl:ObjectProperty rdf:about="&ns;account_of">
        <rdfs:label xml:lang="en">account of</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
        <rdfs:domain rdf:resource="&ns;UserAccount"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://rdfs.org/sioc/ns#has_owner -->

    <owl:ObjectProperty rdf:about="&ns;has_owner">
        <rdfs:label xml:lang="en">has owner</rdfs:label>
        <rdfs:comment xml:lang="en">A UserAccount that this resource is owned by.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
        <rdfs:range rdf:resource="&ns;UserAccount"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#availableIn -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#availableIn">
        <rdfs:label xml:lang="en">available in</rdfs:label>
        <rdfs:comment xml:lang="en">Property to indicate a country supported by a marketplace</rdfs:comment>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Country"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Martkeplace"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#basedOn -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#basedOn">
        <rdfs:label xml:lang="en">based on </rdfs:label>
        <rdfs:comment xml:lang="en">Property to specify the microtasks that were accomplished in a Worker  or Requester Experience</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#RequesterExperience"/>
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#WorkerExperience"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#Microtask"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#belongsTo -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#belongsTo">
        <rdfs:label xml:lang="en">Belongs To</rdfs:label>
        <rdfs:comment xml:lang="en">Relationship that specifies the owner of an item (e.g. the owner of the CV)</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ns;UserAccount"/>
                    <rdf:Description rdf:about="&foaf;Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#byUserAccount -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#byUserAccount">
        <rdfs:label xml:lang="en">by user account</rdfs:label>
        <rdfs:comment xml:lang="en">Property to relate a worker or requester experience with the user account that was used to work or publish work</rdfs:comment>
        <rdfs:range rdf:resource="&ns;UserAccount"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#RequesterExperience"/>
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#WorkerExperience"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#createdBy -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#createdBy">
        <rdfs:label xml:lang="en">Created By</rdfs:label>
        <rdfs:comment xml:lang="en">The user account who created the qualification in the marketplace</rdfs:comment>
        <rdfs:range rdf:resource="&ns;UserAccount"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Qualification"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#domain -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#domain">
        <rdfs:label xml:lang="en">domain</rdfs:label>
        <rdfs:comment xml:lang="en">The domain of microtasks. The subject or subjects described in the ontology.</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasCrowdsourcingRole -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasCrowdsourcingRole">
        <rdfs:label xml:lang="en">Has Role</rdfs:label>
        <rdfs:comment xml:lang="en">The role a crowdsorcing agent has while intereatcing in online labor marketplaces (e.g. requester, crowd worker).</rdfs:comment>
        <rdfs:domain rdf:resource="&ns;UserAccount"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasEvaluation -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasEvaluation">
        <rdfs:label xml:lang="en">Evaluation</rdfs:label>
        <rdfs:comment xml:lang="en">The Evaluation of the owner of the agent. This kind of evaluation does not refer to the accomplishment of a particular microtask or master microtask, but instead it refers to the global evaluation of a crowdsourcing agent (e.g. crowd worker). This evaluation is provided by the marketplace and it is founded on either the workers&#39; opinion -- when a requester is evaluated -- or on the requesters&#39; opinion -- when a crowd worker is evaluated. </rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Evaluation"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasExplicitInterest -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasExplicitInterest">
        <rdfs:label xml:lang="en">Has Explicit Interest</rdfs:label>
        <rdfs:comment xml:lang="en">Property to specify that the CV owner has declared to have interest in a particular topic (e.g. Facebook Likes) </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasImplicitInterest -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasImplicitInterest">
        <rdfs:label xml:lang="en">Has Implicit Interest</rdfs:label>
        <rdfs:comment xml:lang="en">Property to specify that the CV owner has shown interest in a particular topic (e.g. while browsing microtasks in a marketplace)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasMasterMicrotask -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasMasterMicrotask">
        <rdfs:label xml:lang="en">has master microtask</rdfs:label>
        <rdfs:comment xml:lang="en">Property to connect a particular microtask with its master microtask. Usually microtasks are published with a template. The set of microtasks with the same UI, configuration and purpose is called master microtask (e.g. master microtask to identify the sentiment of Tweets), in contrast to particular microtasks (e.g. the microtask to identify the sentiment of the tweet that Angela Merkel posted today). </rdfs:comment>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Microtask"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasPublished -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasPublished">
        <rdfs:label xml:lang="en">Has Published</rdfs:label>
        <rdfs:comment xml:lang="en">The master microtask o particular microtask that the owner of the CV has published</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasQualification -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasQualification">
        <rdfs:label xml:lang="en">Has Qualification</rdfs:label>
        <rdfs:comment xml:lang="en">Qualification related to a microtask or master microtask</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Qualification"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#hasWorkedOn -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#hasWorkedOn">
        <rdfs:label xml:lang="en">has worked on</rdfs:label>
        <rdfs:comment xml:lang="en">Property to relate a Worker Experience to a CV</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#WorkerExperience"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#isEvaluationOf -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#isEvaluationOf">
        <rdfs:label xml:lang="en">is evaluation of</rdfs:label>
        <rdfs:comment xml:lang="en">It connects the evaluation with the user account.</rdfs:comment>
        <rdfs:range rdf:resource="&ns;UserAccount"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Evaluation"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#isLocatedIn -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#isLocatedIn">
        <rdfs:label xml:lang="en">Is Located In</rdfs:label>
        <rdfs:comment xml:lang="en">Property to specify location (i.e. country)</rdfs:comment>
        <rdfs:domain rdf:resource="&ns;UserAccount"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Country"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#isRegisteredAt -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#isRegisteredAt">
        <rdfs:label xml:lang="en">is registered at</rdfs:label>
        <rdfs:comment xml:lang="en">Property that indicates the marketplace where a user account belongs to.</rdfs:comment>
        <rdfs:domain rdf:resource="&ns;UserAccount"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Martkeplace"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#isRelatedtoCompetence -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#isRelatedtoCompetence">
        <rdfs:label xml:lang="en">Is Related To Comptence</rdfs:label>
        <rdfs:comment xml:lang="en">The related competence that is validated by the qualification test</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Qualification"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#obtainedQualification -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#obtainedQualification">
        <rdfs:label xml:lang="en">Obtained Qualification</rdfs:label>
        <rdfs:comment xml:lang="en">Relationship to specify the qualification that has been obtained.</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Qualification"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#providedBy -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#providedBy">
        <rdfs:label xml:lang="en">Provided By</rdfs:label>
        <rdfs:comment xml:lang="en">The Evaluation provider (i.e. the marketplace).</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Evaluation"/>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Martkeplace"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#publishedAt -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#publishedAt">
        <rdfs:label xml:lang="en">Published At</rdfs:label>
        <rdfs:comment xml:lang="en">The online labor marketplace where something (i.e. microtask or master microtask) was published.</rdfs:comment>
        <rdfs:range rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#Martkeplace"/>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#purpose -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#purpose">
        <rdfs:label xml:lang="en">Purpose</rdfs:label>
        <rdfs:comment xml:lang="en">Purpose or main goal of the (master) microtask. The type of task to be carried out (e,g, ImageAnnotation, or Texttranslation)</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#url -->

    <owl:ObjectProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#url">
        <rdfs:label xml:lang="en">URL
</rdfs:label>
        <rdfs:comment xml:lang="en">URL of a Web site</rdfs:comment>
        <rdfs:range rdf:resource="&owl;Thing"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#MasterMicrotask"/>
                    <rdf:Description rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#Qualification"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="&owl;topObjectProperty"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:ObjectProperty rdf:about="&foaf;homepage">
        <rdfs:label>homepage</rdfs:label>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <rdfs:range rdf:resource="&foaf;Document"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="&dc;description">
        <rdfs:label xml:lang="en-us">Description</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/elements/1.1/"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdfs.org/sioc/ns#name -->

    <owl:DatatypeProperty rdf:about="&ns;name">
        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
    </owl:DatatypeProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#accuracy -->

    <owl:DatatypeProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#accuracy">
        <rdfs:label xml:lang="en">Accuracy</rdfs:label>
        <rdfs:comment xml:lang="en">The accuracy the crowd worker showed while accomplishing the microtask specified by the Worker Experience. This value is calculated by the marketplace. </rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#WorkerExperience"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#creationDate -->

    <owl:DatatypeProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#creationDate">
        <rdfs:label xml:lang="en">Creation Date</rdfs:label>
        <rdfs:comment xml:lang="en">The date when the user account was created</rdfs:comment>
        <rdfs:domain rdf:resource="&ns;UserAccount"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://west.uni-koblenz.de/ontologies/crowdworkcv#cvVersion -->

    <owl:DatatypeProperty rdf:about="http://west.uni-koblenz.de/ontologies/crowdworkcv#cvVersion">
        <rdfs:label xml:lang="en">CV Version</rdfs:label>
        <rdfs:comment xml:lang="en">Version of the CV (i.e. whether it is a comprehensive detailed CV or a short CV).</rdfs:comment>
        <rdfs:domain rdf:resource="http://west.uni-koblenz.de/ontologies/crowdworkcv#CV"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">detailed</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">short</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
